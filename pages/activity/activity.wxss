.searchCity {
  width: 100%;
  height: 80rpx;
  border: 1px solid #ebebeb;
  border-radius: 10rpx;
  margin-bottom: 28rpx;
  margin-top: 28rpx;
}

.searchDate {
  width: 100%;
  height: 80rpx;
  border: 1px solid #ebebeb;
  border-radius: 10rpx;
  line-height: 80rpx;
  margin-bottom: 28rpx;
  position: relative;
  margin-top: 28rpx;
}

.hotelNameBox {
  width: 100%;
  height: 80rpx;
  border: 1px solid #ebebeb;
  -webkit-appearance: none;
  border-radius: 10rpx;
  position: relative;
  margin-top: 28rpx;
  margin-bottom: 28rpx;
}

.toSearch {
  width: rem(640);
  height: rem(100);
  background-image: linear-gradient(-90deg, #05afaf 0%, #3cd9d9 98%);
  border-radius: rem(200);
  text-align: center;
  font-size: rem(40);
  line-height: rem(100);
  color: #fff;
  letter-spacing: rem(2);
  margin-top: rem(60);
}

input::-webkit-input-placeholder {
  color: #999;
}

input:-moz-placeholder {
  color: #999;
}

input::-moz-placeholder {
  color: #999;
}

input:-ms-input-placeholder {
  color: #999;
}

.chooseCity {
  font-weight: 500;
  width: 450rpx;
  display: inline-block;
  font-size: 36rpx;
  color: #333;
  padding-left: 20rpx;
  line-height: 80rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.stayCity {
  display: inline-block;
  float: right;
  padding: 10rpx 20rpx;
  margin-top: 8rpx;
  border-left: 1px solid #f2f2f2;
  width: 120rpx;
  text-align: center;
}

.hotelName {
  width: 550rpx;
  margin-top: 25rpx;
  /* height: 80rpx;  */
  line-height: 80rpx;
  font-size: 36rpx;
  color: #333;
  padding-left: 20rpx;
}

.clearHotel {
  position: absolute;
  z-index: 23;
  width: rem(50);
  right: 0;
  margin-top: rem(25);
}

.stayNight {
  font-size: 22rpx;
  line-height: 22rpx !important;
  color: #666;
  letter-spacing: -1rpx;
  text-align: center;
  vertical-align: center;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
}

.stayRight {
  float: right;
  padding-right: 20rpx;
}

.stayCity image {
  width: 20rpx;
  height: 20rpx;
  left: 45rpx;
  top: 15rpx;
  text-align: center;
}

.stayCity p {
  font-size: 20rpx;
  color: #05afaf;
  letter-spacing: 1rpx;
}

.searchDate span {
  display: inline-block;
  font-size: 36rpx;
  color: #333;
  padding-left: 20rpx;
  font-weight: 500;
}

.searchDate span font {
  font-size: 22rpx;
  color: #999;
  letter-spacing: -1rpx;
  padding-left: 10rpx;
}

.commodity_screen {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: #000;
  opacity: 0.6;
  overflow: hidden;
  z-index: 1000;
  color: #fff;
}

.commodity_attr_box {
  width: 100%;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 2000;
  background: #fff;
}

.commodity_attr_box button {
  border: 0px !important;
  font-size: 26rpx !important;
  color: #333 !important;
  padding: 22rpx 0 !important;
}

.commodity_attr_box button:nth-child(2) {
  border-top: 1px solid #e9e9e9 !important;
  border-bottom: 1px solid #e9e9e9 !important;
  border-radius: 0px !important;
}

